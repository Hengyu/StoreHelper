{"schemaVersion":{"patch":0,"minor":3,"major":0},"identifier":{"url":"doc:\/\/StoreHelper\/documentation\/StoreHelper\/StoreHelper\/handleStoreKit1Transactions(productId:date:status:transaction:)","interfaceLanguage":"swift"},"primaryContentSections":[{"declarations":[{"tokens":[{"kind":"attribute","text":"@"},{"kind":"attribute","preciseIdentifier":"s:ScM","text":"MainActor"},{"text":" ","kind":"text"},{"text":"func","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"handleStoreKit1Transactions"},{"text":"(","kind":"text"},{"text":"productId","kind":"externalParam"},{"kind":"text","text":": "},{"identifier":"doc:\/\/StoreHelper\/documentation\/StoreHelper\/ProductId","preciseIdentifier":"s:11StoreHelper9ProductIda","kind":"typeIdentifier","text":"ProductId"},{"kind":"text","text":", "},{"text":"date","kind":"externalParam"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:10Foundation4DateV","text":"Date","kind":"typeIdentifier"},{"kind":"text","text":", "},{"text":"status","kind":"externalParam"},{"kind":"text","text":": "},{"text":"TransactionStatus","kind":"typeIdentifier","preciseIdentifier":"s:11StoreHelper17TransactionStatusO","identifier":"doc:\/\/StoreHelper\/documentation\/StoreHelper\/TransactionStatus"},{"text":", ","kind":"text"},{"text":"transaction","kind":"externalParam"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","text":"SKPaymentTransaction","preciseIdentifier":"c:objc(cs)SKPaymentTransaction"},{"kind":"text","text":") "},{"text":"async","kind":"keyword"}],"languages":["swift"],"platforms":["macOS"]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"name":"productId","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The "},{"type":"codeVoice","code":"ProductId"},{"type":"text","text":" that the transaction relates to."}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The date and time of the transaction update."}]}],"name":"date"},{"content":[{"inlineContent":[{"text":"The new status (subscribed, expired, etc.) of the sunscription.","type":"text"}],"type":"paragraph"}],"name":"status"}]},{"kind":"content","content":[{"text":"Discussion","level":2,"type":"heading","anchor":"discussion"},{"inlineContent":[{"text":"Note that in Xcode StoreKit Testing and Sandbox Testing subscription renewal or cancellation transactions that","type":"text"},{"type":"text","text":" "},{"type":"text","text":"happen when the app’s not running are NEVER picked up by StoreKit2. That is, the transactions don’t appear"},{"type":"text","text":" "},{"type":"text","text":"in "},{"code":"StoreKit.Transaction.all","type":"codeVoice"},{"type":"text","text":" or "},{"type":"codeVoice","code":"Transaction.currentEntitlement(for:)"},{"text":". This seems to have been a known issue","type":"text"},{"type":"text","text":" "},{"text":"since the release of StoreKit2 and can lead to the situation where a user has paid to renew their subscription","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"but StoreKit2 has no knowledge of it."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"As a workaround, "},{"type":"codeVoice","code":"StoreHelper"},{"text":" maintains a ","type":"text"},{"code":"transactionUpdateCache","type":"codeVoice"},{"type":"text","text":" that keeps track of subscription renewals"},{"type":"text","text":" "},{"text":"that happen when the app’s not running. See ","type":"text"},{"type":"codeVoice","code":"AppStoreHelper.paymentQueue(_:updatedTransactions:)"},{"text":".","type":"text"}]},{"inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"Note that production builds using the live App Store DO NOT appear to suffer from this issue"}]},{"type":"text","text":"."}],"type":"paragraph"}]}],"metadata":{"modules":[{"name":"StoreHelper"}],"externalID":"s:11StoreHelperAAC06handleA16Kit1Transactions9productId4date6status11transactionySS_10Foundation4DateVAA17TransactionStatusOSo09S